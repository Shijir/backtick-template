{
  "name": "backtick-template",
  "version": "0.1.0",
  "description": "ES2015 backticks for ES3+ engines",
  "main": "backtick-template.js",
  "scripts": {
    "test": "node test.js",
    "size": "cat backtick-template.js | wc -c;cat backtick-template.min.js | wc -c;gzip -c backtick-template.min.js | wc -c",
    "minify": "uglifyjs backtick-template.js --comments=all --compress --mangle -o backtick-template.min.js",
    "cover": "istanbul cover test.js",
    "coveralls": "cat ./coverage/lcov.info | coveralls"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/WebReflection/backtick-template.git"
  },
  "keywords": [
    "backtick",
    "ES6",
    "ES2015",
    "string",
    "template"
  ],
  "author": "Andrea Giammarchi",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/WebReflection/backtick-template/issues"
  },
  "homepage": "https://github.com/WebReflection/backtick-template#readme",
  "devDependencies": {
    "consolemd": "^0.1.2",
    "coveralls": "^2.11.15",
    "istanbul": "^0.4.5",
    "uglify-js": "^2.7.5"
  }
}
